!Xft.dpi:   112
!Xft.antialias:   1
!Xft.rgba:   none
!Xft.hinting:   1
!Xft.hintstyle:   hintfull
!Xft.autohint:   1
!URxvt.imlocale: en_US.UTF-8
!URxvt.font: xft:FiraCode-Regular:size=14
!URxvt.boldFont: xft:FiraCode-Bold:size=16
!URxvt.scrollBar: false
!URxvt.perl-ext-common:clipboard,matcher,font-size,url-select,resize-font
!URxvt.ulr-launcher: /usr/local/bin/xdg-open
!URxvt.matcher.button: 1
!URxvt*depth: 32
!URxvt.background: [95]#000000
!URxvt.foreground: #40a62f
!URxvt.shading:5
!URxvt*cursorBlink: 1
!URxvt.iso14755: false
!URxvt.iso14755_52: false
!URxvt*loginShell: true
!URxvt.cursorColor: magenta
